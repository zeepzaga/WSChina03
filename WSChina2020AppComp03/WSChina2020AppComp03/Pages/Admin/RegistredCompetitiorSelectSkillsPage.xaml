<Page x:Class="WSChina2020AppComp03.Pages.Admin.RegistredCompetitiorSelectSkillsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSChina2020AppComp03.Pages.Admin"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RegistredCompetitiorSelectSkillsPage">

    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <TextBlock Style="{StaticResource StyleTblTitle}" HorizontalAlignment="Center"> Select SKills</TextBlock>
            <StackPanel Orientation="Horizontal">
                <TextBlock Style="{StaticResource StyleTbBody}" VerticalAlignment="Center">Skill:</TextBlock>
                <ComboBox DisplayMemberPath="FullCompetition" Name="CbSkills" Width="400"></ComboBox>
            </StackPanel>
            <Button x:Name="BtnOk" Click="BtnOk_Click" Margin="5" Height="30" HorizontalAlignment="Right" Width="150">Ok</Button>
        </StackPanel>
    </Grid>
</Page>
