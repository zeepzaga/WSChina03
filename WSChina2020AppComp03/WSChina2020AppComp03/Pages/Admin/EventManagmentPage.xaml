<Page x:Class="WSChina2020AppComp03.Pages.Admin.EventManagmentPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WSChina2020AppComp03.Pages.Admin"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EventManagmentPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Style="{StaticResource StyleTblTitle}" HorizontalAlignment="Center" Margin="30">Event Managment</TextBlock>
        <StackPanel Grid.Row="1" VerticalAlignment="Center">
            <Button x:Name="BtnEvent" Click="BtnEvent_Click" Margin="10" Height="60" Width="300">Competition Event</Button>
            <Button x:Name="BtnComp" Click="BtnComp_Click"  Margin="10" Height="60" Width="300">Competiton Skills</Button>
        </StackPanel>
    </Grid>
</Page>
